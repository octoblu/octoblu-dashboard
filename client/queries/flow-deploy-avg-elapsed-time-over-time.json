{
  "aggs": {
    "finished": {
      "filter": {
        "range": {
          "beginTime": {
            "gte": 0
          }
        }
      },
      "aggs": {
        "startTime_over_time": {
          "date_histogram": {
            "field": "beginTime",
            "interval": "hour",
            "format": "MM/dd HH:00"
          },
          "aggs": {
            "avgElapsedTime": {
              "avg": {
                "field": "elapsedTime"
              }
            }
          }
        }
      }
    }
  }
}
